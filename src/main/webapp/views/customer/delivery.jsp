<!-- reservation-section 
			================================================== -->
<section class="reservation-section">
	<div class="container">
		<div class="item-content">
			<div class="row">
				<div class="col-lg-8">
					<div class="buttons-holder">
						<a class="button-one" href="#">Quick Delivery</a> <a
							class="button-one" href="#">Have Account</a>
					</div>
					<form id="reservation-form">
						<input name="address" id="address" type="text"
							placeholder="Enter Street Address"> <input name="room"
							id="room" type="text" placeholder="Appartment, Suite or Room |">
						<div class="row">
							<div class="col-md-4">
								<input name="city" id="city" type="text" placeholder="City">
							</div>
							<div class="col-md-4">
								<input name="state" id="state" type="text" placeholder="State">
							</div>
							<div class="col-md-4">
								<input name="zip-code" id="zip-code" type="text"
									placeholder="Zip Code">
							</div>
						</div>
						<input type="submit" id="submit_reservation"
							value="Make Your Order">
						<p>Our orders take from 5 - 10 minutes to reach destionation,
							depending on the locaiton of the buyer</p>
					</form>
				</div>
			</div>
		</div>
	</div>
	<div class="image-holder">
		<img src="${pageContext.servletContext.contextPath}/resources/upload/others/a6.png" alt="">
	</div>
</section>
<!-- End reservation section -->

